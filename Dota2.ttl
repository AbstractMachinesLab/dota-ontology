#################################################################

###  https://abstractmachines.dev/wittgenstein/Dota2#DotaObjectProperty
:DotaObjectProperty rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:comment "The Top Object Property that all properties in the Dota ontology must subclass."@en ;
                    rdfs:label "Dota Object Property"@en .


###  https://abstractmachines.dev/wittgenstein/Dota2#castingType
:castingType rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :DotaObjectProperty ;
             rdfs:domain :Ability ;
             rdfs:range :CastingType .


###  https://abstractmachines.dev/wittgenstein/Dota2#damageType
:damageType rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :DotaObjectProperty ;
            rdfs:domain :Ability ;
            rdfs:range :DamageType .


###  https://abstractmachines.dev/wittgenstein/Dota2#targetingType
:targetingType rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :DotaObjectProperty ;
               rdfs:domain :Ability ;
               rdfs:range :TargetingType .


#################################################################
#    Data properties
#################################################################

###  https://abstractmachines.dev/wittgenstein/Dota2#DotaDataProperty
:DotaDataProperty rdf:type owl:DatatypeProperty ;
                  rdfs:comment "The Top Data Property that all data properties in the Dota ontology must subclass."@en ;
                  rdfs:label "Dota Data Property"@en .


###  https://abstractmachines.dev/wittgenstein/Dota2#castRange
:castRange rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :DotaDataProperty ;
           rdfs:domain :Ability ;
           rdfs:range xsd:positiveInteger ;
           rdfs:comment "The maximum distance at which a spell can be casted on its target."@en .


###  https://abstractmachines.dev/wittgenstein/Dota2#linkBreakDistance
:linkBreakDistance rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :DotaDataProperty ;
                   rdfs:domain :Ability ;
                   rdfs:range xsd:positiveInteger ;
                   rdfs:comment "Abiliities that link units together have a limit distance between these units before the link is broken and the ability is cancelled or dispelled."@en .


###  https://abstractmachines.dev/wittgenstein/Dota2#maximumChannelingTime
:maximumChannelingTime rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :DotaDataProperty ;
                       rdfs:domain :Ability ;
                       rdfs:range xsd:positiveInteger ;
                       rdfs:comment "Channeled abilities can be channeled for this maximum amount of seconds."@en .


###  https://abstractmachines.dev/wittgenstein/Dota2#movementSpeed
:movementSpeed rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :DotaDataProperty ;
               rdfs:domain :Hero ;
               rdfs:range xsd:positiveInteger ;
               rdfs:comment "The speed at which a unit can move."@en .



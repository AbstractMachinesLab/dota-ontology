###  https://abstractmachines.dev/wittgenstein/Dota2#castingType
:castingType rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Ability ;
             rdfs:range :CastingType .


###  https://abstractmachines.dev/wittgenstein/Dota2#damageType
:damageType rdf:type owl:ObjectProperty ;
            rdfs:domain :Ability ;
            rdfs:range :DamageType .


###  https://abstractmachines.dev/wittgenstein/Dota2#targetingType
:targetingType rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Ability ;
               rdfs:range :TargetingType .


#################################################################
#    Data properties
#################################################################

###  https://abstractmachines.dev/wittgenstein/Dota2#castRange
:castRange rdf:type owl:DatatypeProperty ;
           rdfs:domain :Ability ;
           rdfs:range xsd:positiveInteger .


###  https://abstractmachines.dev/wittgenstein/Dota2#linkBreakDistance
:linkBreakDistance rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:positiveInteger .


###  https://abstractmachines.dev/wittgenstein/Dota2#maximumChannelingTime
:maximumChannelingTime rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:positiveInteger .


#################################################################
#    Classes
#################################################################

###  https://abstractmachines.dev/wittgenstein/Dota2#Ability
:Ability rdf:type owl:Class ;
         rdfs:subClassOf :DotaThing .


###  https://abstractmachines.dev/wittgenstein/Dota2#CastingType
:CastingType rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( :Channeled
                                             )
                                 ] ;
             rdfs:subClassOf :DotaThing .


###  https://abstractmachines.dev/wittgenstein/Dota2#DamageType
:DamageType rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :MagicalDamage
                                            )
                                ] ;
            rdfs:subClassOf :DotaThing .


###  https://abstractmachines.dev/wittgenstein/Dota2#DotaThing
:DotaThing rdf:type owl:Class ;
           rdfs:comment "A Thing in the Dota Ontology"@en .


###  https://abstractmachines.dev/wittgenstein/Dota2#TargetingType
:TargetingType rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( :UnitTarget
                                               )
                                   ] ;
               rdfs:subClassOf :DotaThing .


#################################################################
#    Individuals
#################################################################

###  https://abstractmachines.dev/wittgenstein/Dota2#Channeled
:Channeled rdf:type owl:NamedIndividual ,
                    :CastingType .


###  https://abstractmachines.dev/wittgenstein/Dota2#LifeDrain
:LifeDrain rdf:type owl:NamedIndividual ;
           :castingType :Channeled ;
           :damageType :MagicalDamage ;
           :targetingType :UnitTarget ;
           :castRange "700"^^xsd:positiveInteger ;
           :linkBreakDistance "900"^^xsd:positiveInteger ;
           :maximumChannelingTime "10"^^xsd:positiveInteger ;
           rdfs:comment """When cast on an enemy, Pugna drains health from the target enemy unit to heal himself and granting vision over the target. If Pugna has full HP, and the enemy target is a Hero, Life Drain will restore mana instead.

When cast on an ally, Pugna will drain his own health into his ally."""@en ;
